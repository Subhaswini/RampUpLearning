@model MyMVCApplication.Models.Travel

@{
    ViewBag.Title = " Tourist Details";
}

<h2>Tourist Details</h2>
<table style="font-family:Arial">
    <tr>
        <td>
            <b>Tourist Id: </b>
        </td>
        <td>
            @Model.TouristId.ToString()
        </td>
    </tr>
    <tr>
        <td>
            <b>Name: </b>
        </td>
        <td>
            @Model.Name
        </td>
    </tr>
    <tr>
        <td>
            <b>Gender: </b>
        </td>
        <td>
            @Model.Gender
        </td>
    </tr>
    <tr>
        <td>
            <b>City: </b>
        </td>
        <td>
            @Model.City
        </td>
    </tr>
</table>
